{
	"name": "nopedotavi",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Redirects malicious links to safety. I hope",
	"browser_action": {
		"default_icon":"img/ico.png",
		"default_popup":"nopedotavi.html"
	},
	"background": {
    "scripts": ["hijack.js"],
    "persistent": false
  },
	"permissions": [
		"activeTab", "webNavigation",  "tabs",
        "notifications",
        "http://*/",
        "https://*/"
	]
}
